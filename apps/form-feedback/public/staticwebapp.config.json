{
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "Cache-Control": "public, max-age=31536000",
        "Content-Type": "application/javascript; charset=utf-8"
      }
    },
    {
      "route": "/assets/*.css",
      "headers": {
        "Cache-Control": "public, max-age=31536000",
        "Content-Type": "text/css; charset=utf-8"
      }
    },
    {
      "route": "/assets/*.map",
      "headers": {
        "Cache-Control": "no-cache",
        "Content-Type": "application/json; charset=utf-8"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "/se/*",
      "rewrite": "/index.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/en/*",
      "rewrite": "/index.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/se",
      "rewrite": "/index.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/en",
      "rewrite": "/index.html",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https://portal.husqvarnagroup.com; img-src 'self' data: https://portal.husqvarnagroup.com; connect-src 'self' https://*.azurestaticapps.net https://*.azurecontainerapps.io;"
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript", 
    ".css": "text/css",
    ".json": "application/json",
    ".html": "text/html",
    ".svg": "image/svg+xml"
  }
}
