{
  "routes": [
    {
      "route": "/assets/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*.js",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.mjs",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.css",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "route": "/se/*",
      "rewrite": "/index.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/en/*",
      "rewrite": "/index.html",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.js", "/*.mjs", "/*.css", "/*.json", "/*.svg", "/*.ico"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https://portal.husqvarnagroup.com; img-src 'self' data: https://portal.husqvarnagroup.com; connect-src 'self' https://*.azurestaticapps.net https://*.azurecontainerapps.io;"
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript", 
    ".css": "text/css",
    ".json": "application/json",
    ".html": "text/html",
    ".svg": "image/svg+xml"
  }
}
