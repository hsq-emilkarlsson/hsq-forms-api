{
  "routes": [
    {
      "route": "/en/*",
      "serve": "/index.html",
      "statusCode": 200,
      "headers": { "Content-Type": "text/html" }
    },
    {
      "route": "/se/*",
      "serve": "/index.html",
      "statusCode": 200,
      "headers": { "Content-Type": "text/html" }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200,
      "headers": { "Content-Type": "text/html" }
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/index.html"
    }
  ]
}
