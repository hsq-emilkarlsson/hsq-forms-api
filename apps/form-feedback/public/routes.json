{
  "routes": [
    {
      "route": "/*.js",
      "headers": { "Content-Type": "application/javascript" }
    },
    {
      "route": "/assets/*.js",
      "headers": { "Content-Type": "application/javascript" }
    },
    {
      "route": "/*.css",
      "headers": { "Content-Type": "text/css" }
    },
    {
      "route": "/assets/*.css",
      "headers": { "Content-Type": "text/css" }
    },
    {
      "route": "/en/*",
      "serve": "/index.html",
      "statusCode": 200,
      "headers": { "Content-Type": "text/html" }
    },
    {
      "route": "/se/*",
      "serve": "/index.html",
      "statusCode": 200,
      "headers": { "Content-Type": "text/html" }
    },
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200,
      "headers": { "Content-Type": "text/html" }
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/index.html"
    }
  ]
}
