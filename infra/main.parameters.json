{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "projectName": {
      "value": "hsq-forms"
    },
    "dbAdminUsername": {
      "value": "${DB_ADMIN_USERNAME}"
    },
    "dbAdminPassword": {
      "value": "${DB_ADMIN_PASSWORD}"
    },
    "containerAppScale": {
      "value": {
        "minReplicas": 1,
        "maxReplicas": 3
      }
    }
  }
}
